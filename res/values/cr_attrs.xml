<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ListPreferenceMultiSelect">
    	<attr format="string" name="checkAll" />
    	<attr format="string" name="separator" />
    </declare-styleable>

    <declare-styleable name="CheckboxSetting">
        <attr name="descriptionOn" format="string"/>
        <attr name="descriptionOff" format="string"/>
    </declare-styleable>

    <declare-styleable name="DeveloperPreference">
        <attr name="devIcon" format="reference" />
        <attr name="devUrl" format="string" />
        <attr name="deviceStat" format="string" />
        <attr name="donateLink" format="string" />
        <attr name="emailDev" format="string" />
        <attr name="githubLink" format="string" />
        <attr name="googleHandle" format="string" />
        <attr name="googleProfile" format="string" />
        <attr name="nameDev" format="string" />
        <attr name="statusDev" format="string" />
        <attr name="twitterLink" format="string" />
    </declare-styleable>

    <declare-styleable name="SeekBarPreference">
        <attr name="maximum" format="integer" />
        <attr name="minimum" format="integer" />
        <attr name="unitsLeft" format="string" />
        <attr name="unitsRight" format="string" />
        <attr name="interval" format="integer" />
    </declare-styleable>
</resources>
